{
  "name": "vanilla-ts-starter",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "repository": "git@github.com:martinmckenna/vanilla-ts-starter.git",
  "author": "mmckenna <mmckenna.phila@gmail.com>",
  "engines": {
    "node": ">= 16.3.0"
  },
  "license": "MIT",
  "private": true,
  "keywords": [
    "javscript",
    "typescript"
  ],
  "scripts": {
    "build": "tsc && webpack",
    "start": "concurrently \"tsc -w --pretty\" \"webpack-dev-server\" -n 'tsc,webpack' -k",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.js\"",
    "typecheck": "tsc --noEmit true --emitDeclarationOnly false",
    "lint": "eslint . --ext '.ts,.tsx'"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@webpack-cli/serve": "^1.6.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "lint-staged": "^11.2.6",
    "node-forge": "^1.3.0",
    "prettier": "^2.2.1",
    "selfsigned": "^1.10.8",
    "style-loader": "^1.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "^3.7.2",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typecheck"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint",
      "prettier --write"
    ]
  }
}
